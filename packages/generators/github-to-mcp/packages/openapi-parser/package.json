{
  "name": "@github-to-mcp/openapi-parser",
  "version": "1.0.0",
  "description": "Multi-format API specification to MCP tools converter",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.1.0",
    "graphql": "^16.8.1",
    "handlebars": "^4.7.8",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.1",
    "zod": "^4.3.5"
  }
}
